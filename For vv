#include <iostream>

int main() {
    // Definir un vector de tama√±o 5
    int vector[5];

    // Utilizar un ciclo FOR para llenar el vector
    for(int i = 0; i < 5; i++) {
        std::cout << "Ingrese el elemento " << i + 1 << ": ";
        std::cin >> vector[i];
    }

    // Mostrar el vector por pantalla
    std::cout << "El vector ingresado es: ";
    for(int i = 0; i < 5; i++) {
        std::cout << vector[i] << " ";
    }
    std::cout << std::endl;

    return 0;
}
