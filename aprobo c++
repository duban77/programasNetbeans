#include<iostream>
using namespace std;

int a,b,c,pro;
int main()
{
	cout<<" ingrese la primera nota:  ";
	cin>>a;
	cout<<" ingrese la segunda nota:  ";
	cin>>b;
	cout<<" ingrese la tercera nota:  ";
	cin>>c;
	pro=((a+b+c)/3);
	if((pro>=30)and(pro<35))
	cout <<"Aprobo, puedes mejorar"<<endl;
	else 
	{
		if((pro>=35)and(pro<40))
		cout <<"Aprobo, Felicitaciones"<<endl;
		else
		
	}	
}
